<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="debug" name="scripter" packages="">
	<Appenders>
		<File name="LELog" fileName="${sys:user.home}/logs/scripter.log" append="false">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} %p %m%n" />
		</File>
		<!-- <Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%m%n" />
		</Console> -->
	</Appenders>
	<Loggers>
		<Root level="DEBUG">
			<AppenderRef ref="LELog" />
		</Root>
	</Loggers>
</Configuration>